{
  "$schema": "http://json.schemastore.org/template",
  "author": "judescripts@gmail.com",
  "classifications": ["Blazor", "Tab"],
  "identity": "DevToys.Tab",
  "name": "DevToys.io templates: TAB template",
  "shortName": "tab",
  "sourceName": "tab",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "customName": {
      "type": "parameter",
      "replaces": "tab",
      "replacementName": "name",
      "fileRename": "tab"
    }
  },
  "postActions": [
    {
      "actionId": "3E8BCBF0-D631-45BA-B12F-13BFBFC7A534",
      "description": "Update project name in solution files.",
      "manualInstructions": [
        {
          "text": "Please run 'update-project-name.ps1' in PowerShell to update project name in solution files."
        }
      ],
      "executable": "powershell",
      "args": "-NoProfile -ExecutionPolicy Bypass -File \"./installation-cleanup.ps1\"",
      "continueOnError": false
    }
  ]
}
